/mm {
  720 mul 224 div
} def

/onebit {
  dup 2 div truncate 2 mul sub
} def

/dropbit {
  2 div truncate
} def

/box {
  save
  5 1 roll
  4 2 roll
  translate
  scale
  newpath
  0 0 moveto
  0 1 rlineto
  1 0 rlineto
  0 -1 rlineto
  closepath
  fill
  restore
} def

/bardot {
  mm 0 mm 1 mm 1 mm box
} def

/barpair {

} def

/barcode {
  save 
  exch
  1 0 1 setrgbcolor
  0 bardot
  3 bardot
  0 0 0 setrgbcolor
  1 bardot
  1 1 1 setrgbcolor
  2 bardot
  dup
  /num 4 def
  8 {
    dup onebit dup dup setrgbcolor
    dropbit
    num bardot
    /num num 1 add def
  } repeat
  pop
  /num -1 def
  8 {
    dup onebit 1 exch sub dup dup setrgbcolor
    dropbit
    num bardot
    /num num 1 sub def
  } repeat
  pop
  restore 
} def

20 mm 10 mm translate
/code 0 def
23 { 
  gsave
  6 {
    code barcode
    25 mm 0 mm translate 
    /code code 1 add def
  } repeat
  grestore
  0 mm 10 mm translate 
} repeat

