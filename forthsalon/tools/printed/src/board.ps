  (21)
  (22)
  (23)
  (24)
  (25)
] def

/code 0 def

/gimme {
  labels code get
  /code code 1 add def
} def

/square {
  gsave
  newpath
  0 0 moveto
  side 0 lineto
  side side lineto
  0 side lineto
  0 0 lineto
  0.1 setlinewidth
  stroke
  grestore
  gimme textface
} def

/fillsquare {
  save
  0.95 0.95 0.95 setrgbcolor
  newpath
  0 0 moveto
  side 0 lineto
  side side lineto
  0 side lineto
  0 0 lineto
  0.1 setlinewidth
  fill
  restore
} def

/column {
  gsave
  rows {
    square
    side 0 translate
  } repeat
  grestore
} def

/grid {
  gsave
  columns {
    column
    0 side translate
  } repeat
  grestore
} def

/haikucol {
  gsave
  {
    fillsquare
    0 side translate
  } repeat
  grestore
} def

/haiku {
  gsave
  side 2 mul side translate
  5 haikucol
  side side -1 mul translate
  7 haikucol
  side side translate
  5 haikucol
  grestore
} def

/board {
  haiku
  grid
  0 0 dotat
  0 columns 1 sub  dotat
  rows 1 sub 0 dotat
  rows 1 sub columns 1 sub dotat
} def

60 150 translate
board

%%EOF
